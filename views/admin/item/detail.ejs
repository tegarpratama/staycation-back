<div class="tab-pane fade show active" id="show-item" role="tabpanel" aria-labelledby="home-tab">
   <div class="card shadow mb-4 mt-2">
      <div class="card-body">
         <div class="row">
            <% for( let i = 0; i < item.imageId.length; i++ ) { %>
               <div class="col">
                  <img src="http://localhost:3000/<%= item.imageId[i].imageUrl %>" alt="image" class="img-fluid">
               </div>
            <% } %>
         </div>
      </div>
   </div>
   <a href="/admin/item" class="btn btn-secondary">Back</a>
</div>
 